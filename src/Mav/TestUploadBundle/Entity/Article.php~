<?php
namespace Mav\TestUploadBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Article
 * 
 * @ORM\Table()
 * @ORM\Entity() 
 */
class Article 
{
    /**
     *
     * @var integer
     * 
     * @ORM\Column(type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    protected $id;
    
    /**
     *
     * @var string
     * 
     * @ORM\Column(type="string")
     */
    protected $title;
    
    /**
     *
     * @var text
     * 
     * @ORM\Column(type="text")
     */
    protected $description;
}